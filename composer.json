{
    "name": "vault/programming-test",
    "description": "Vault Innovation Programming Test",
    "keywords": ["programming", "test"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Vault\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/InterviewTests.php"
        ]
    },
    "scripts": {
        "pre-update-cmd": [
            "composer dump-autoload -o"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
